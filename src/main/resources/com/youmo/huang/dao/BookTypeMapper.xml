<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.youmo.huang.dao.BookTypeMapper">

    <select id="getOne" resultType="com.youmo.huang.bean.BookType">
        SELECT
            NO,
            NAME
        FROM
            booktype
        WHERE
            `no` = #{no}

    </select>

    <select id="getList" resultType="com.youmo.huang.bean.BookType">
        SELECT
            NO,
            NAME
        FROM
            booktype
    </select>

</mapper>